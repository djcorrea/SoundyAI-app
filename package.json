{
  "name": "chatbot-correa",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vercel dev",
    "dev:upload": "node dev-server.js",
    "start": "node work/server.js",
    "worker": "node work/worker-redis.js",
    "start:web": "node work/server.js",
    "start:worker": "node work/worker-redis.js",
    "build:refs": "node tools/reference-builder.js",
    "validate:refs": "node tools/reference-validator.js",
    "calibrate:refs": "node tools/ref-calibrator.js",
    "refs:pipeline": "node tools/refs-pipeline.js",
    "copy:refs": "node tools/copy-refs-to-public.js",
    "metrics:recalc": "node tools/metrics-recalc.js",
    "audit:one": "AUDIT_MODE=1 node tools/audit-one.js",
    "audit:batch": "AUDIT_MODE=1 node tools/audit-batch.js",
    "audit:report": "AUDIT_MODE=1 node tools/audit-report.js",
    "audit:lufs": "node scripts/manual-audit-lufs.js",
    "audit:truepeak": "node scripts/manual-audit-truepeak.js",
    "audit:spectral": "node scripts/manual-audit-spectral.js",
    "audit:scoring": "node scripts/manual-audit-scoring.js",
    "audit:audio": "node scripts/manual-audit-lufs.js && node scripts/manual-audit-truepeak.js && node scripts/manual-audit-spectral.js && node scripts/manual-audit-scoring.js",
    "audit:serve": "python -m http.server 3000",
    "test-job": "curl -X POST http://localhost:3000/api/audio/analyze -H 'Content-Type: application/json' -d '{\"fileKey\":\"uploads/test.wav\",\"mode\":\"genre\"}'",
    "postinstall": "npm run copy:refs"
  },
  "dependencies": {
    "audio-decode": "^2.2.3",
    "aws-sdk": "^2.1692.0",
    "bullmq": "^5.61.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.4.1",
    "fluent-ffmpeg": "^2.1.3",
    "formidable": "^3.5.4",
    "ioredis": "^5.8.2",
    "mercadopago": "^2.8.0",
    "multer": "^2.0.2",
    "music-metadata": "^11.8.3",
    "music-tempo": "^1.0.3",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3"
  },
  "main": "work/server.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "madge": "^8.0.0",
    "serve": "^14.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djcorrea/prod.ai-novo.git"
  },
  "bugs": {
    "url": "https://github.com/djcorrea/prod.ai-novo/issues"
  },
  "homepage": "https://github.com/djcorrea/prod.ai-novo#readme",
  "descriptionn": ""
}
